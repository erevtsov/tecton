    "version": "0.2.0",
    "configurations": [
        {
            "name": "dagster dev",
            "type": "debugpy",
            "request": "launch",
            "module": "dagster",
            "args": [
                "dev",
            ],
            "subProcess": true
        },        
        {
            "name": "dagster-uv",
            "type": "debugpy",
            "request": "launch",
            "program": "~/.local/bin/uv",
